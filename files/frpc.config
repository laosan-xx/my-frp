config init
	option stdout 1
	option stderr 1
	option user frpc
	option group frpc
	option respawn 1
#	OS environments pass to frp for config file template, see
#	https://github.com/fatedier/frp#configuration-file-template
#	list env 'ENV_NAME=value'
#	Config files include in temporary config file.
#	list conf_inc '/etc/frp/frpc.d/frpc_full.ini'

config conf 'common'
	option server_addr frp.2026178.xyz
	option server_port 5443
	option token TiZTjCmJ9ZwCCiMy
	option user 0000
	option login_fail_exit false
	option protocol websocket
	option tls_enable false
#	List options with name="_" will be directly appended to config file
#	list _ '# Key-A=Value-A'

config conf 'ssh'
	option type tcp
	option local_ip 192.168.100.1
	option local_port 22

config conf 'web'
	option type tcp
	option use_encryption true
	option use_compression true
	option local_ip 127.0.0.1
	option local_port 80
